{
  "name": "jsdom-extra",
  "version": "3.0.6",
  "description": "JSDOM with extra tweak ( jquery / cheerio / request )",
  "keywords": [
    "agent",
    "browser",
    "cheerio",
    "cookie",
    "data",
    "document",
    "dom",
    "form",
    "get",
    "html",
    "htmlparser",
    "http",
    "javascript",
    "jquery",
    "jsdom",
    "library",
    "parser",
    "post",
    "query",
    "request",
    "scraper",
    "selector",
    "simple",
    "url",
    "util",
    "utility",
    "w3c",
    "whatwg",
    "window"
  ],
  "homepage": "https://github.com/bluelovers/jsdom-extra#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/jsdom-extra/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/jsdom-extra.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "test": "yarn run test:mocha",
    "test:mocha": "ynpx mocha -- --require ts-node/register \"!(node_modules)/**/*.test.{ts,tsx}\"",
    "npm:publish": "npm publish",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly": "yarn run test",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "@bluelovers/request-promise": "^4.2.5",
    "@jsdom-extra/cookie": "^1.0.1",
    "@jsdom-extra/html-util": "^1.0.3",
    "@jsdom-extra/jsdom-to-query": "^1.0.3",
    "@jsdom-extra/resource-loader": "^1.0.3",
    "@jsdom-extra/virtual-console": "^1.0.1",
    "array-hyper-unique": "^1.1.9",
    "bluebird": "^3.7.2",
    "class-proxy": "^1.1.2",
    "content-type-parser": "^1.0.2",
    "deepmerge-plus": "^2.1.3",
    "html-encoding-sniffer": "^2.0.1",
    "html-minifier": "^4.0.0",
    "is-plain-object": "^3.0.1",
    "jquery": "^3.5.1",
    "jsdom": "16.3.0",
    "jsdom-url": "^3.0.1",
    "lazy-cookies": "^1.0.23",
    "moment": "^2.27.0",
    "request-cached": "^1.0.1",
    "whatwg-encoding": "^1.0.5"
  },
  "devDependencies": {},
  "peerDependencies": {
    "request": "^2.88.2",
    "tough-cookie": "*"
  },
  "gitHead": "863aeb0af329745ccde219f103b7ab0c70e4e272"
}
