{
  "name": "jsdom-extra",
  "version": "3.0.2",
  "description": "JSDOM with extra tweak ( jquery / cheerio / request )",
  "keywords": [
    "agent",
    "browser",
    "cheerio",
    "cookie",
    "data",
    "document",
    "dom",
    "form",
    "get",
    "html",
    "htmlparser",
    "http",
    "javascript",
    "jquery",
    "jsdom",
    "library",
    "parser",
    "post",
    "query",
    "request",
    "scraper",
    "selector",
    "simple",
    "url",
    "util",
    "utility",
    "w3c",
    "whatwg",
    "window"
  ],
  "homepage": "https://github.com/bluelovers/jsdom-extra#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/jsdom-extra/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/jsdom-extra.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "npm:publish": "npm publish",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly": "yarn run test",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "sort-package-json": "npx yarn-tool sort",
    "test": "ynpx mocha -- --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "@bluelovers/request-promise": "^4.2.5",
    "array-hyper-unique": "^1.1.0",
    "bluebird": "^3.7.2",
    "class-proxy": "^1.1.2",
    "content-type-parser": "^1.0.2",
    "deepmerge-plus": "^2.1.3",
    "html-encoding-sniffer": "^2.0.1",
    "html-minifier": "^4.0.0",
    "is-plain-object": "^3.0.0",
    "jquery": "^3.4.1",
    "jsdom": "16.2.2",
    "jsdom-url": "^3.0.1",
    "lazy-cookies": "^1.0.18",
    "moment": "^2.24.0",
    "request-cached": "^1.0.1",
    "whatwg-encoding": "^1.0.5"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.19",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/html-minifier": "^3.5.3",
    "@types/jquery": "^3.3.34",
    "@types/jsdom": "^16.2.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.46",
    "@types/tough-cookie": "^2.3.6",
    "chai": "^4.2.0"
  },
  "peerDependencies": {
    "request": "^2.88.2",
    "tough-cookie": "*"
  },
  "optionalDependencies": {
    "jquery": "^3.4.1"
  }
}
